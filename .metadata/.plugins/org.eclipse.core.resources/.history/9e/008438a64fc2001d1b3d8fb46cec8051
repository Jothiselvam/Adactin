package com.maths;

import java.util.Scanner;

public class Switch_Task {
	public static void main(String[] args) {
		for(int i=1;i<=12;i++) {
			System.out.println("Select the month 1.January 2.February 3.March 4.April 5.May 6.June 7.July");
			Scanner a=new Scanner(System.in);
			
			int month=a.nextInt();
			System.out.println(month);
			
			
			
			
			
			
			
		}
		
		
		
		
		
		
		
	}

}
